{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "@services/*": [
        "src/app/core/services/*"
      ],
      "@models/*": [
        "src/app/core/models/*"
      ],
      "@interfaces/*": [
        "src/app/core/interfaces/*"
      ],
      "@enums/*": [
        "src/app/core/enums/*"
      ],
      "@shared/*": [
        "src/app/shared/*"
      ]
    }
  }
}